<div class="login_form">
  <h1>Instagramc</h1>
  <%= form_with(scope: :session, url:  sessions_path, local: true) do |form|%>
    <p><%= form.email_field :email, placeholder: 'email' %></p>
    <p><%= form.password_field :password, placeholder: 'password' %></p>
    <div class="login_form_button">
      <%= form.submit "Login" %>
    </div>
  <% end %>
</div>
